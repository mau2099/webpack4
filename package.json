{
  "name": "Playground-Webpack",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:byCLI": "webpack --entry ./index.js --output ./bundle.js --mode development",
    "build:byWebpackConfig": "webpack --mode production",
    "build:external": "webpack --config ./external/webpack_external.config.js --mode production",
    "build:multi": "webpack --config ./multi/webpack.config.js --mode production",
    "build:css": "webpack --config ./css-style-loader/webpack.config.js --mode development",
    "build:plugins": "webpack --config ./plugins/webpack.config.js --mode production",
    "build:dev": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js  --mode production",
    "build:babel": "webpack-dev-server --config ./babel/webpack.config.js  --mode development --watch",
    "build:react": "webpack-dev-server --config ./react/webpack.config.js  --mode development --watch",
    "build:url-loader": "webpack-dev-server --config ./url-loader/webpack.config.js  --mode development --watch",
    "build:prepro": "webpack-dev-server --config ./url-loader/webpack.config.js  --mode development --watch",
    "build:optimization": "webpack --config ./optimization/webpack.config.js  --watch"
  },
  "keywords": [],
  "author": "@mau2099.dev",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.9.6",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-runtime": "7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "7.9.4",
    "babel-loader": "^8.3.0",
    "css-loader": "6.7.3",
    "file-loader": "6.0.0",
    "html-webpack-plugin": "5.5.0",
    "mini-css-extract-plugin": "2.7.2",
    "style-loader": "1.2.1",
    "url-loader": "4.1.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "3.10.3"
  }
}
